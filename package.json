{
  "version": "6.0.0",
  "name": "fortnox-react-client-template",
  "private": true,
  "scripts": {
    "dev": "TRANS_EXTRACT=1 TRANS_LOCALE=sv_SE node ./webpack/devserver.js",
    "dev-branding": "BRANDING=1 TRANS_EXTRACT=1 TRANS_LOCALE=sv_SE node ./webpack/devserver.js",
    "dev-en": "TRANS_EXTRACT=1 TRANS_LOCALE=en_GB node ./webpack/devserver.js",
    "devwindows": "SET TRANS_EXTRACT=1&& SET TRANS_LOCALE=sv_SE&& node ./webpack/devserver.js",
    "devwindows-branding": "SET BRANDING=1&& SET TRANS_EXTRACT=1&& SET TRANS_LOCALE=sv_SE&& node ./webpack/devserver.js",
    "prod-translate-sv": "TRANS_EXTRACT=1 TRANS_LOCALE=sv_SE NODE_ENV=production webpack --config ./webpack/config.js",
    "prod-translate-en": "TRANS_EXTRACT=1 TRANS_LOCALE=en_GB NODE_ENV=production webpack --config ./webpack/config.js",
    "extract-merge-translations": "TRANS_EXTRACT=1 TRANS_LOCALE=sv_SE NODE_ENV=production webpack --config ./webpack/config.js; TRANS_EXTRACT=1 TRANS_LOCALE=en_GB NODE_ENV=production webpack --config ./webpack/config.js; msgmerge i18n/en_GB.po i18n/translations.pot -U; msgmerge i18n/sv_SE.po i18n/translations.pot -U",
    "prod": "BRANDING=1 TRANS_EXTRACT=1 TRANS_LOCALE=sv_SE NODE_ENV=production webpack --config ./webpack/config.js",
    "prodwindows": "SET BRANDING=1&& SET TRANS_EXTRACT=1&& SET TRANS_LOCALE=sv_SE&& SET NODE_ENV=production&& webpack --config ./webpack/config.js",
    "test": "NODE_ENV=test NODE_PATH=./src:$NODE_PATH:./node_modules/fortnox-react/src mocha test/**/*.test.jsx",
    "merge-translations": "msgmerge i18n/en.po i18n/translations.pot -U && msgmerge i18n/sv.po i18n/translations.pot -U",
    "postinstall": "sh ./node_modules/fortnox-eslint-codestandard/upgrade-latest.sh"
  },
  "dependencies": {
    "backbone": "1.2.3",
    "c-3po": "0.5.5",
    "font-awesome": "4.6.1",
    "history": "2.1.1",
    "lesshint-webpack-plugin": "https://github.com/TobiasFriberg/lesshint-webpack-plugin.git#v0.0.4",
    "prop-types": "15.5.9",
    "react": "16.2.0",
    "null-loader": "0.1.1",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-c-3po": "0.5.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.4",
    "enzyme": "2.8.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-loader": "0.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "*",
    "fortnox-eslint-codestandard": "git+ssh://git@git.fnox.se:7999/fnx/fortnox-eslint.git#master",
    "fortnox-react": "git+ssh://git@git.fnox.se:7999/fnx/fortnox-react.git#v8.0.2",
    "frontend-branding": "git+ssh://git@git.fnox.se:7999/fnx/frontend-branding.git#develop",
    "imports-loader": "0.7.1",
    "jsdom": "9.12.0",
    "less": "2.7.2",
    "less-loader": "4.0.4",
    "lesshint": "4.6.3",
    "mocha": "3.0.0",
    "postcss-loader": "2.0.6",
    "prepend-loader": "0.0.2",
    "react-addons-test-utils": "15.5.1",
    "react-hot-loader": "1.3.1",
    "should": "11.1.2",
    "should-enzyme": "1.2.0",
    "style-loader": "*",
    "url-loader": "*",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-notifier": "1.3.0"
  }
}
